            },
            {
              "id": "c_le1b",
              "left": { "type": "price", "source": "close" },
              "operator": "gt",
              "right": { "type": "variable", "variableId": "var_kcUpper" }
            }
          ]
        }
      ],
      "exitSettings": { "takeProfitEnabled": false, "stopLossEnabled": false }
    },
    "longExit": {
